# --- 基础运行配置 ---
cron: false
data_dir: "data"
end_date: ""  # 留空代表运行到今天

# --- 核心：股票名单 ---
# 注意：短横线 - 前面必须有两个空格
codes:
  - '002050.SZ'

# --- 推送配置 (PushPlus 专用) ---
push:
  enable: true
  
  # 关键点：这里留空即可，GitHub Actions 会自动把你存的 Token 填进去
  pushplus_token: ""
  
  # 下面这两个留空，保留它们是为了防止程序报错说“找不到字段”
  wxpusher_uid: ""
  wxpusher_token: ""
